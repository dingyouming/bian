{
  "manifest_version": 3,
  "name": "Binance Margin Balance",
  "version": "1.0",
  "description": "查询币安保证金账户余额",
  "permissions": ["storage"],
  "host_permissions": [
    "https://api.binance.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}